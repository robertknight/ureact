{
  "name": "ureact",
  "version": "0.1.0",
  "description": "Small React-like UI library",
  "type": "module",
  "repository": "https://github.com/robertknight/ureact",
  "author": "Robert Knight <robertknight@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "c8": "^7.4.0",
    "chai": "^4.2.0",
    "jsdom": "^16.4.0",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^9.2.2",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "tsc",
    "package": "rollup -c",
    "checkformatting": "prettier --check **/*.{js,ts}",
    "format": "prettier --list-different --write **/*.{js,ts}",
    "test": "c8 mocha"
  },
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./enzyme": "./dist/enzyme.js",
    "./hooks": "./dist/index.js",
    "./jsx-runtime": "./dist/jsx-runtime.js",
    "./jsx-dev-runtime": "./dist/jsx-dev-runtime.js",
    "./test-utils": "./dist/test-utils.js"
  }
}
